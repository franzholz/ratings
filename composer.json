{
  "name": "netcreators/ratings",
  "type": "typo3-cms-extension",
  "description": "Shop system extension for TYPO3 CMS",
  "keywords": [
    "TYPO3",
    "extension",
    "rating"
  ],
  "support": {
    "issues": "http://typo3.slack.com/messages/typo3-cms"
  },
  "homepage": "https://www.netcreators.nl/",
  "authors": [
    {
      "name": "Jordi Bakker",
      "role": "Developer"
    }
  ],
  "license": [
    "GPL-2.0+"
  ],
  "require": {
    "typo3/cms-core": ">=7.0.0,<8.99.99",
    "typo3-ter/div2007": ">=1.10.1"
  },
  "autoload": {
    "psr-4": {
      "Netcreators\\Ratings\\": "Classes"
    }
  },
  "replace": {
    "ratings": "self.version",
    "typo3-ter/ratings": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "ratings"
    }
  }
}
